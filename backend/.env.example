# Server Configuration
PORT=3000
NODE_ENV=development
CORS_ORIGIN=*

# Hugging Face Configuration
HUGGINGFACE_ACCESS_TOKEN=your_huggingface_token
HUGGINGFACE_MODEL=stabilityai/stable-diffusion-2

# Story Protocol Configuration
PRIVATE_KEY=your_private_key
RPC_URL=https://sepolia.base.org
CHAIN_ID=84532
SPG_NFT_CONTRACT=your_spg_nft_contract
ROYALTY_POLICY=your_royalty_policy_contract
WIP_TOKEN=your_wip_token_address
NFT_CONTRACT_ADDRESS=your_nft_contract_address

# IPFS Configuration
IPFS_API_KEY=your_ipfs_api_key # Optional, for Pinata or similar service
IPFS_API_SECRET=your_ipfs_api_secret # Optional, for Pinata or similar service
IPFS_GATEWAY=https://ipfs.io/ipfs/ # Default IPFS gateway
IPFS_DATA_DIR=data/ipfs # Local directory for IPFS data
IPFS_PIN_METADATA=true # Whether to pin metadata by default
IPFS_TIMEOUT_MS=30000 # Timeout for IPFS operations in milliseconds
IPFS_MAX_RETRIES=3 # Maximum number of retry attempts for IPFS operations

# Wallet Configuration
WALLET_PRIVATE_KEY=your_wallet_private_key
RPC_PROVIDER_URL=your_rpc_provider_url